{#
/**
 * @file
 * Macro for embedding svg icons
 *
 * @var string name
 *   The name of the svg to embed. It should match one of the svgs
 *   that exist within the icons/assets/ directory.
 */
#}
{% macro get(icon, iconClass) %}
  {% include '@finenothings/icons/assets/' ~ icon ~ '.svg.twig' with {
    'icon': icon,
    'iconClass': iconClass
    } only
  %}
{% endmacro %}
