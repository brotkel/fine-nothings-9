{%
  embed "@finenothings/code/code.twig" with {
    "filename": content.field_filename|render,
    "code": content.field_code,
    "language": content.field_language,
    "extra_attributes": attributes|without(class)
  } only
%}
  {% block code %}
    {{ code }}
  {% endblock code %}
{% endembed %}